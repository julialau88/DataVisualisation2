{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": "container",
  "height": 250,
  "config": {
    "rect": {"strokeWidth": 5},
      "view": {
    
          "step": 13
      },
      "axis": {
          "domain": false
      }
  },
  "data": {
    "url": "https://raw.githubusercontent.com/julialau88/DataVisualisation2/main/data/patient_gender_my.csv"
  },
  "transform": [
    {"calculate": "ceil (datum.id/10)", "as": "col"},
    {"calculate": "datum.id - datum.col*10", "as": "row"}
  ],
  "mark": {"type": "rect", "filled": true},
  "encoding": {
    "x": {"field": "col", "type": "ordinal", "axis": null,     "scale": {
      "paddingInner": 0.09,
      "paddingOuter": 0
    }},
    "y": {"field": "row", "type": "ordinal", "axis": null,     "scale": {
      "paddingInner": 0.09,
      "paddingOuter": 0
    }},
    "color": {
      "field": "gender",
      "scale":{"range": ["rgb(255, 187, 51)", "rgb(255, 71, 26)"]},
      "title": "Gender"
    
    },
    "size": {"value":200}
  }
}
